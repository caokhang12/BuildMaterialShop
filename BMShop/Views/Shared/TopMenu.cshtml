@model List<Model.EF.Menu>
<div class="call">
    <p><span>Cần giúp?</span> Gọi cho chúng tôi <span class="number">123456789</span></span></p>
</div>
<div class="account_desc">
    @{
        var session = (BMShop.Common.UserLogin)Session[BMShop.Common.CommonConstants.USER_SESSION];
    }
    @if (session == null)
    {
        <ul>
            @foreach (var item in Model)
            {
                <li><a href="@item.Link" target="@item.Target">@item.Text</a></li>
            }
        </ul>
    }
    else
    {
        <ul>
            <li>Xin chào: @session.UserName</li>
            <li>@Html.ActionLink("Thoát", "Logout", "User")</li>

        </ul>
    }
</div>
<div class="clear"></div>
